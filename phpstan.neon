parameters:
    level: 8
    paths:
        - Api
        - Checks
        - Cron
        - Model
        - HealthCheckRegistry.php
        - registration.php
    ignoreErrors:
        - '~Parameter\s+\#[0-9]+\s+\$[A-Za-z0-9_]+\s+of method\s+.+::__construct\(\)\s+is not optional\.~'
    reportUnmatchedIgnoredErrors: false
    bootstrapFiles:
        - phpstan-bootstrap.php
